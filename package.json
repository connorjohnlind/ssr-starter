  "engines": {
    "node": "9.2.0",
    "npm": "5.6.0"
  },
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "axios": "0.18.0",
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-plugin-syntax-dynamic-import": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-polyfill": "*",
