    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node build/server-bundle.js",
    "build": "NODE_ENV=production npm-run-all --parallel prod:build-server prod:build-client",
    "dev": "NODE_ENV=development npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --inspect build/server-bundle.js",
    "dev:build-server": "webpack --config webpack.config.dev-server.js --watch",
    "dev:build-client": "webpack --config webpack.config.dev-client.js --watch",
    "prod": "heroku local -p 8080",
    "prod:build-server": "rimraf build && webpack --config webpack.config.prod-server.js",
    "prod:build-client": "rimraf public && webpack --config webpack.config.prod-client.js",
    "heroku-postbuild": "npm run build"
  }
}
